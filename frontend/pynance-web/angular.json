{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pynance-web": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../build",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              {
                "glob": "index.html",
                "input": "src/assets/docs/",
                "output": "/docs"
              },
              {
                "glob": "*",
                "input":"src/assets/docs",
                "ignore": ["index.html"],
                "output":"/"
              },
              {
                "glob": "**/*",
                "input": "src/assets/docs",
                "output": "/"
              },
              {
                "glob": "favicon.ico",
                "input": "src/",
                "output": "/"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "src/styles.css"
            ],
            "scripts": [
              { "input": "src/assets/docs/_static/doctools.js",
                "bundleName": "sphinx-docs"},
              { "input": "src/assets/docs/_static/documentation_options.js",
                "bundleName": "sphinx-docs"},
              { "input": "src/assets/docs/_static/jquery-3.5.1.js",
                "bundleName": "sphinx-docs"},
              { "input": "src/assets/docs/_static/jquery.js",
                "bundleName": "sphinx-docs"},
              { "input": "src/assets/docs/_static/language_data.js",
                "bundleName": "sphinx-docs"},
              { "input": "src/assets/docs/_static/searchtools.js",
                "bundleName": "sphinx-docs"},
              { "input": "src/assets/docs/_static/underscore-1.12.0.js",
                "bundleName": "sphinx-docs"},
              { "input": "src/assets/docs/_static/underscore.js",
                "bundleName": "sphinx-docs"}
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pynance-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pynance-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pynance-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "pynance-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "pynance-web:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "pynance-web"
}